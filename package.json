{
  "name": "ReactNativePlayground",
  "author": "Salih Gürkan Süerdem",
  "license": "MIT",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:avd": "emulator -avd Nexus_5X_API_23",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:devices": "adb devices",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "adb shell input keyevent 82",
    "avd": "emulator -avd Nexus_5X_API_23",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "flow:update": "flow-typed install --overwrite",
    "generate": "plop --plopfile generators/index.js",
    "git-hook": "npm test -s",
    "lint": "esw src --color --quiet",
    "lint:commit": "esw webpack.config.* src --color --quiet --rule '{no-console: 2}'",
    "lint:fix": "npm run lint -- --fix",
    "lint:unused": "esw webpack.config.* src --color --quiet --rule '{no-unused-vars: 2}'",
    "lint:watch": "npm run lint -- --watch",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean && npm i",
    "XXXXXXXXXprecommit": "npm run git-hook",
    "XXXXXXXXXprepush": "npm run git-hook",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:watch": "jest --watch",
    "typings": "typings install",
    "updateSnapshot": "jest --updateSnapshot"
  },
  "dependencies": {
    "apisauce": "^0.13.0",
    "axios": "^0.16.2",
    "format-json": "^1.0.3",
    "lodash": "^4.17.2",
    "prop-types": "^15.5.10",
    "querystringify": "0.0.4",
    "ramda": "^0.24.1",
    "react": "~15.4.1",
    "react-dom": "~15.4.1",
    "react-native": "~0.42.3",
    "react-native-config": "^0.2.1",
    "react-native-device-info": "^0.10.0",
    "react-native-drawer": "^2.3.0",
    "react-native-extended-stylesheet": "^0.5.0",
    "react-native-push-notification": "^3.0.0",
    "react-native-vector-icons": "^4.2.0",
    "react-navigation": "^1.0.0-beta.7",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-form": "^6.7.0",
    "redux-form-saga": "^0.1.0",
    "redux-ignore": "^1.2.4",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.1.0",
    "redux-saga": "^0.14.3",
    "reduxsauce": "0.4.1",
    "remote-redux-devtools": "^0.5.11",
    "seamless-immutable": "^7.0.1"
  },
  "devDependencies": {
    "autobind-decorator": "^1.4.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "20.0.3",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^1.9.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx": "^0.0.2",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-lodash": "^2.4.2",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-react-native": "^2.3.2",
    "eslint-plugin-redux-saga": "^0.3.0",
    "eslint-watch": "^3.1.0",
    "flow-bin": "^0.47.0",
    "handlebars": "^4.0.10",
    "husky": "^0.13.1",
    "ignite-ir-boilerplate": "^2.0.0-rc.6",
    "jest": "20.0.4",
    "mockery": "^2.0.0",
    "plop": "^1.8.0",
    "prompt": "^1.0.0",
    "react-addons-test-utils": "^15.3.1",
    "react-test-renderer": "~15.4.1",
    "reactotron-react-native": "^1.11.1",
    "reactotron-redux": "^1.11.1",
    "reactotron-redux-saga": "^1.11.1"
  },
  "jest": {
    "testMatch": [
      "**/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "Tests/Setup.js"
    ],
    "setupFiles": [
      "./Tests/Setup"
    ],
    "preset": "react-native"
  },
  "config": {},
  "//": [
    "yarn add redux-form redux-form-saga -S",
    "yarn add redux-logger redux-immutable-state-invariant -D",
    "yarn add axios -S",
    "yarn add react-native-extended-stylesheet -S",
    "yarn add react-native-push-notification react-native-vector-icons -S",
    "yarn add handlebars plop prompt -D",
    "yarn add flow-bin -D",
    "yarn add babel-eslint eslint eslint-plugin-babel eslint-plugin-flowtype eslint-plugin-import eslint-plugin-jsx eslint-plugin-jsx-a11y eslint-plugin-lodash eslint-plugin-react eslint-plugin-react-native eslint-plugin-redux-saga eslint-watch -D",
    "yarn add babel-preset-es2015 autobind-decorator babel-preset-react babel-preset-stage-0 babel-preset-stage-1 babel-preset-stage-2 babel-plugin-transform-decorators babel-plugin-transform-decorators-legacy babel-plugin-transform-function-bind -D"
  ]
}
